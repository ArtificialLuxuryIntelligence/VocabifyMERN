{
  "name": "vocabify",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm install && node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.11",
    "morgan": "~1.9.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.2"
  }
}
